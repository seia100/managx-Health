// authService.ts
import api from './api';

export const login = async (email: string, contrase単a: string) => {
    const response = await api.post('/auth/login', { email, contrase単a });
    return response.data;
};

export const register = async (nombre: string, email: string, contrase単a: string, rol: string) => {
    const response = await api.post('/auth/register', { nombre, email, contrase単a, rol });
    return response.data;
};
